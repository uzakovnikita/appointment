{
  "name": "appointment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "migrate:up": "prisma migrate dev",
    "migrate:down": "prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-migrations prisma/migrations --shadow-database-url postgresql://admin:password@localhost:5434/shadow?schema=public --script > down.sql",
    "migrate:dev": "npm run migrate:down && npm run migrate:up",
    "migrate:prod": "prisma migrate deploy",
    "db:reset": "npx prisma migrate reset"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.3.1",
    "@prisma/client": "^6.5.0",
    "@react-input/mask": "^2.0.4",
    "@reduxjs/toolkit": "^2.7.0",
    "@tanstack/react-form": "^1.9.0",
    "@tanstack/react-query": "^5.74.7",
    "ky": "^1.8.1",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.1",
    "react-redux": "^9.2.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "prisma": "^6.5.0",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "node ./prisma/seed.js"
  }
}
